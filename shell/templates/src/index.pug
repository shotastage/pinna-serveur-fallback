extends _base.pug


block contents

    #root
    #maps

    style.
        #maps {
            height: 400px;
        }

    script(src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA32MTfuMesMcdn8CyF4wjcdizPKbqUdVg" async defer)
    script.
        function initMap() {
            var mapPosition = {lat: 35.170662, lng: 136.923430}
            var mapArea = document.getElementById('maps');
            var mapOptions = {
                center: mapPosition,
                zoom: 16,
            };
                var map = new google.maps.Map(mapArea, mapOptions);
        }
